{
  "uid": "705ab4f5-6393-11e8-b7cc-42010a800002",
  "kind": {
    "group": "batch",
    "version": "v1",
    "kind": "CronJob"
  },
  "resource": {
    "group": "batch",
    "version": "v1",
    "resource": "cronjobs"
  },
  "requestKind": {
    "group": "batch",
    "version": "v1",
    "kind": "CronJob"
  },
  "requestResource": {
    "group": "batch",
    "version": "v1",
    "resource": "cronjobs"
  },
  "name": "nginx-cronjob",
  "namespace": "default",
  "operation": "CREATE",
  "userInfo": {
    "groups": [
      "system:masters"
    ],
    "username": "test-user"
  },
  "object": {
    "apiVersion": "batch/v1",
    "kind": "CronJob",
    "metadata": {
      "name": "nginx-cronjob",
      "namespace": "default"
    },
    "spec": {
      "jobTemplate": {
        "spec": {
          "template": {
            "spec": {
              "containers": [
                {
                  "command": [
                    "/bin/sh",
                    "-c",
                    "echo \"Running nginx in cronjob\""
                  ],
                  "image": "nginx:1.21",
                  "name": "nginx-container",
                  "resources": {
                    "limits": {
                      "cpu": "500m",
                      "memory": "256Mi"
                    },
                    "requests": {
                      "cpu": "100m",
                      "memory": "128Mi"
                    }
                  }
                }
              ],
              "restartPolicy": "OnFailure"
            }
          }
        }
      },
      "schedule": "0 2 * * *"
    }
  }
}
